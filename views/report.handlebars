<script>

  $(document).ready(function () {


  // Getting jQuery references to the post user rbpm, bmi, blog, mood

var rbpmInput = $("#RBPM");
var bmiInput = $("#BMI");
var blogInput = $("#body");
var moodInput = $("#theZone");

    $("#checkinSubmitButton").on("click", function (event) {

      // Need this to stop page reloading.
      event.preventDefault();
      console.log("checkinSubmitButton button clicked.");


      // Bogus data.
      let putObj = {
        day_id: {{ data.day_id }},
        rbpm: parseInt($("#RBPM").val()),
        bmi:parseFloat($("#BMI").val()),
        blog: $("#body").val(),
        mood: $("input[name='optradio']:checked").val() 
      };

      console.log(putObj);
      

      // Send the PUT request.
      $.ajax("/api/update_metrics", {
        type: "PUT",
        data: putObj
      }).then(
        function () {
          // Load the home page.
          console.log("calling /");
          window.location = "/";
        }
      );

    });

  });

</script>