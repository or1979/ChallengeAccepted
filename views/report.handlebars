    </div>

    <br>

    <!-- The Zone -->
    <div class="zoneform" align="center">
        <h2>What Zone Are You In?</h2>
        <div class="form-check" id="theZone">
            <div class="radio">
            <label><input type="radio" name="optradio" value="0"><button type="button" class="btn btn-info" data-toggle="modal" data-target="#bluez" >Blue Zone</button></label>
            </div>
            <div class="radio">
            <label><input type="radio" name="optradio" value="1"><button type="button" class="btn btn-success" data-toggle="modal" data-target="#greenz">Green Zone</button></label>
            </div>
            <div class="radio">
            <label><input type="radio" name="optradio" value="2"><button type="button" class="btn btn-warning" data-toggle="modal" data-target="#yellowz">Yellow Zone</button></label>
            </div>
            <div class="radio">
            <label><input type="radio" name="optradio" value="3"><button type="button" class="btn btn-danger" data-toggle="modal" data-target="#redz">Red Zone</button></label>
            </div>
            
        </div>
    </div>

    <br>

    <!--Blog-->
    <div class="blogger">
        <form id="fitBlog">
          <div class="form-group">

            <br />
            <label for="body">Blog about it!</label>
            <textarea class="form-control" rows="10" id="body"></textarea>
            <div class="form-group">
              
            </div>
            <br />
            <button type="submit" class="btn btn-success submit btn-lg" id="checkinSubmitButton">Submit</button>
          </div>
        </form>
    </div>

    <!-- Modals -->

    <!-- rbpm -->
    <div class="modal fade" id="rbpm" tabindex="-1" role="dialog" aria-labelledby="exampleModalScrollableTitle" aria-hidden="true">
