<div class="container">

    <p>Day {{data.day_id}} Check In</p>

    <br>

         <!--RBPM -->
    <div class="restingbeats">
        <input type="number" class="userData" id="RBPM" name="#" placeholder="What's your RBPM?">
        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#rbpm">
            RBPM
        </button>
    </div>

        <br>
        <br>

        <!-- BMI -->
    <div class="bodymass">
        <input type="number" class="userData" id="BMI" name="#" placeholder="What's your BMI?">
